{
	"name": "pp_loadAPIdata_toSQLserver_2",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"paginationRules": {
							"supportRFC5988": "true"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false,
						"tableOption": "autoCreate",
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['status']"
								},
								"sink": {
									"name": "status",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['id']"
								},
								"sink": {
									"name": "id",
									"type": "Int64"
								}
							},
							{
								"source": {
									"path": "['employee_name']"
								},
								"sink": {
									"name": "employee_name",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['employee_salary']"
								},
								"sink": {
									"name": "employee_salary",
									"type": "Int64"
								}
							},
							{
								"source": {
									"path": "['employee_age']"
								},
								"sink": {
									"name": "employee_age",
									"type": "Int64"
								}
							},
							{
								"source": {
									"path": "['profile_image']"
								},
								"sink": {
									"name": "profile_image",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['message']"
								},
								"sink": {
									"name": "message",
									"type": "String"
								}
							}
						],
						"collectionReference": "$['data']"
					}
				},
				"inputs": [
					{
						"referenceName": "ds_restapi",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "ds_JsonApi",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}