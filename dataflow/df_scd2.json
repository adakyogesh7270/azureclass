{
	"name": "df_scd2",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_dynamic_file_name",
						"type": "DatasetReference"
					},
					"name": "sourceemp1csv"
				},
				{
					"dataset": {
						"referenceName": "ds_dynamicsql",
						"type": "DatasetReference"
					},
					"name": "sourceemptable"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_dynamicsql",
						"type": "DatasetReference"
					},
					"name": "sinkemptableinsert"
				},
				{
					"dataset": {
						"referenceName": "ds_dynamicsql",
						"type": "DatasetReference"
					},
					"name": "sinkupdate"
				}
			],
			"transformations": [
				{
					"name": "derivedColumnisactive1"
				},
				{
					"name": "lookup1"
				},
				{
					"name": "derivedColumnisActive0"
				}
			],
			"scriptLines": [
				"source(output(",
				"          id as string,",
				"          name as string,",
				"          city as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> sourceemp1csv",
				"source(output(",
				"          id as string,",
				"          name as string,",
				"          city as string,",
				"          isActive_1 as integer,",
				"          modified_date as timestamp",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> sourceemptable",
				"sourceemp1csv derive(isActive_1 = 1,",
				"          modified_date_0 = currentUTC()) ~> derivedColumnisactive1",
				"sourceemp1csv, sourceemptable lookup(sourceemp1csv@id == sourceemptable@id,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup1",
				"lookup1 derive(isActive_0 = 0) ~> derivedColumnisActive0",
				"derivedColumnisactive1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          id as string,",
				"          name as string,",
				"          city as string,",
				"          isActive_1 as integer,",
				"          modified_date as timestamp",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     recreate:true,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     saveOrder: 2) ~> sinkemptableinsert",
				"derivedColumnisActive0 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          id as string,",
				"          name as string,",
				"          city as string,",
				"          isActive_1 as integer,",
				"          modified_date as timestamp",
				"     ),",
				"     deletable:false,",
				"     insertable:false,",
				"     updateable:true,",
				"     upsertable:false,",
				"     keys:['id'],",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     saveOrder: 1,",
				"     mapColumn(",
				"          id = sourceemptable@id,",
				"          name = sourceemptable@name,",
				"          city = sourceemptable@city,",
				"          isActive_1 = isActive_0,",
				"          modified_date",
				"     )) ~> sinkupdate"
			]
		}
	}
}